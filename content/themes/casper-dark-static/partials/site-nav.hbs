<nav class="site-nav">
    <div class="site-nav-left">
        {{^is "home"}}
            {{#if @site.logo}}
                <a class="site-nav-logo" href="{{@site.url}}"><img src="{{@site.logo}}" alt="{{@site.title}}" /></a>
            {{else}}
                <a class="site-nav-logo" href="{{@site.url}}">{{@site.title}}</a>
            {{/if}}
        {{/is}}
        {{#if @site.navigation}}
            {{navigation}}
        {{/if}}
    </div>
    <div class="site-nav-right">
        <div class="social-links">

                <a class="social-link social-link-fb" href="https://store.steampowered.com/developer/ZetsubouGames" title="Steam" target="_blank" rel="noopener"><img src="/content/images/2019/06/steam_bw.png" style="height:16px;"/></a>
                <a class="social-link social-link-fb" href="https://zetsuboushita.itch.io/" title="Itch.io" target="_blank" rel="noopener"><img src="/content/images/2019/06/itch_bw.png" style="height:16px;"/></a>
                <a class="social-link social-link-fb" href="https://social.librem.one/@zetsubougames" title="Mastodon" target="_blank" rel="noopener"><img src="/content/images/2019/06/mastodon_white.png" style="height:16px;"/></a>

                
            {{#if @site.twitter}}
                <a class="social-link social-link-tw" href="{{twitter_url @site.twitter}}" title="Twitter" target="_blank" rel="noopener">{{> "icons/twitter"}}</a>
            {{/if}}
        </div>
        {{#if @labs.subscribers}}
            <a class="subscribe-button" href="#subscribe">Subscribe</a>
        {{else}}
            <a class="rss-button" href="{{@site.url}}/rss/" title="RSS" target="_blank" rel="noopener">{{> "icons/rss"}}</a>
        {{/if}}
    </div>
</nav>
